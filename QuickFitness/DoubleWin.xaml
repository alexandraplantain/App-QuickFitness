<Window x:Class="QuickFitness.DoubleWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickFitness"
        mc:Ignorable="d"
        Title="DoubleWin" Height="830" Width="1284" MouseLeftButtonDown="Window_MouseLeftButtonDown" Window.ResizeMode = "NoResize" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="background_basic.png"/>
        </Grid.Background>
        <Button x:Name="Back_to_choose_login" BorderBrush="{x:Null}" Height="55" Click="Button_go_to_all_train" Margin="25,18,1204,741" Width="55">
            <Button.Background>
                <ImageBrush ImageSource="button_back.png"/>
            </Button.Background>
            


        </Button>
        <Button x:Name="button_close"  Width="30" HorizontalAlignment="Right" Height="30" Margin="0,0,0,0" VerticalAlignment="Top" Click="Button_Click_Close">
            <Button.Background>
                <ImageBrush ImageSource="button_close.png"/>
            </Button.Background>


        </Button>
        <Frame Content="" x:Name="Main_Frame" NavigationUIVisibility="Hidden" Margin="0,102,0,0" Width="1284"/>



    </Grid>
</Window>
